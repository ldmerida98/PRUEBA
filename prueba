#include<stdio.h>

int main(){
	int calificacion;
	
	printf("Escribe por favor tu calificacion obtenida: ");
	scanf("%i",&calificacion);	
	printf("\n\n\n");
if(calificacion>=0 && calificacion<=100){
		if(calificacion<69){
			printf("Tu calificacion obtenida es F");
		}
		else if(calificacion==69){
			printf("Tu calificacion obtenida es D");
		}
		else if(calificacion>=70 && calificacion<=79){
			printf("Tu calificacion obtenida es C");
		}
		else if(calificacion>=80 && calificacion<=89){
			printf("Tu calificacion obtenida es B");
		}
		else if(calificacion>=90 && calificacion<=100){
			printf("Tu calificacion obtenida es A");
		}
	}
	else{
	printf("Por favor valida tu calificacion porque esta incorrecta");
	} 
	
	getch();
	return 0;
}

